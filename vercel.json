{
  "rewrites": [
    { 
      "source": "/Agents/:path*", 
      "destination": "http://127.0.0.1:11000/Agents/:path*" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "create-react-app"
}
